@using CodeGenSite.Utils
@{
    Layout = null;
}
<!DOCTYPE html>
<html itemscope itemtype="http://schema.org/Product">
<head>
    <link href='http://fonts.googleapis.com/css?family=Bree+Serif|Alex+Brush' rel='stylesheet'
        type='text/css'>
    <meta charset="utf-8">
    <title>@ViewBag.PageTitle</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="CodeGenerate.me is a free online service that lets you generate QR Codes and Barcodes of all types. Use this easy service to generate all your barcode needs!">
    <meta name="keywords" content="Free, Online, Barcode, Dean Hume, Qr Codes" />
    <meta name="author" content="">
     <link rel="shortcut icon" href="@Url.Content(string.Format("{0}{1}","~/content/img/favicon.ico?v=",@Html.GetVersion()))" />
    <!-- Le Scripts -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <!-- Le Styles -->
    <link rel="stylesheet" href="/content/css/bootstrap.css?v=@Html.GetVersion()">
    <link rel="stylesheet" href="/content/css/bootstrap-responsive.css?v=@Html.GetVersion()">
    <style>
        body
        {
            padding-top: 0px;
        }
    </style>
</head>
<body>
    <div class="container">
        <!-- Header -->
        <div class="row-fluid">
            <div class="span5" style="padding-top: 50px">
                <div style="z-index: 2; position: absolute;">
                    <a class="logo" style="" href="/">CodeGenerate.me</a>
                </div>
                <div class="logo_me">
                    <span style="">me</span>
                </div>
            </div>
            <div class="span7" style="padding-top: 50px">

            </div>
            <div id="env" style="position: fixed; right: 20px; top: 0;">
                <a href="/Code/Saved">
                    <img src="/content/img/saved.png?v=@Html.GetVersion()" alt="My Saved Codes"></a>
            </div>
        </div>
        <div style="border-bottom: 1px solid #FAFBFC; border-top: 1px solid #EAF2F8; height: 0;
            margin: 19px 0 0; width: 100%;">
        </div>
        <br />
        <!-- Intro Text -->
        <div class="row">
            <div class="span4 offset4">
                <p align="center" style="font-size: 22px;">
                    Create QR codes and Barcodes on the go - Quickly and easily.</p>
                <p align="center" style="font-size: 14px;">
                    <span style="color: #51A351">Just choose a barcode type below and hit enter.</span>
                </p>
            </div>
        </div>
        <br />
        <!-- Elements -->
        <div class="row">
            <div class="span4 offset4">
                <p align="center">
                    <select id="barcodeType" name="barcodeType" class="inset-ddlinput">
                        <option value="">Select a Barcode Type</option>
                        <option value="qrcode">QR Code</option>
                        <option value="code128a">Code128 A</option>
                        <option value="code128b">Code128 B</option>
                        <option value="code128c">Code128 C</option>
                        <option value="code11">Code 11</option>
                        <option value="code39">Code 39</option>
                        <option value="ean13">EAN 13</option>
                        <option value="ean8">EAN 8</option>
                        <option value="isbn">ISBN</option>
                        <option value="bookland">Bookland</option>
                    </select>
                </p>
                <p align="center">
                    <input type="text" id="barcodeValue" class="inset-textinput" placeholder="Enter text...." /></p>
                <br />
                <p align="center">
                    <input type="button" class="btn btn-large btn-success" data-toggle="button" id="CreateButton"
                        value="Create Code" /></p>
                <br />
                <br />
                <!-- Alert -->
                <div class="alert alert-error" id="alertBox">
                </div>
                <!-- Progress bar -->
                <div class="progress progress-striped active" id="progressBar">
                    <div class="bar" style="width: 40%;">
                    </div>
                </div>
                <!-- Image -->
                <div id="image" align="center">
                </div>
            </div>
            <br />
        </div>
        <script src="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/bootstrap/js")"></script>
        <div style="border-bottom: 1px solid #FAFBFC; border-top: 1px solid #EAF2F8; height: 0;
            margin: 19px 0 0; width: 100%;">
        </div>
@*        <footer class="footer">
                <div class="row">
                    <div class="span2">
                        <ul class="unstyled">
                            <li style="font-weight: bold;">CodeGenerate.me</li>
                            <li><a href="@Url.Action("Contact")">Contact</a></li>
                            <li><a href="http://www.twitter.com/CodeGenerate">Twitter</a></li>
                        </ul>
                    </div>
                    <div class="span4">
                        <div class="span4">
                            <ul class="unstyled">
                                <li style="font-weight: bold;">About</li>
                                <li><a href="@Url.Action("API")">API</a></li>
                                <li><a href="@Url.Action("About")">About us</a></li>
                                <li><a href="https://plus.google.com/114790681908422991984?rel=author" title="Google+" alt="Google+" rel="author">Designed by</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="span1 offset4">
                        <img src="@Url.Content("~/content/img/azure-logo.png")" alt="Powered by Windows Azure">
                    </div>
                </div>
            </footer>*@
    </div>
</body>
<!-- Le Analytics -->
<script type="text/javascript">
    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-30376104-1']);
    _gaq.push(['_trackPageview']);
    _gaq.push(['_trackPageLoadTime']);

    (function () {
        // Analytics
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        // Plus one
        var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
        po.src = 'https://apis.google.com/js/plusone.js';
        var puss = document.getElementsByTagName('script')[0]; puss.parentNode.insertBefore(po, puss);
    })();
</script>
</html>
